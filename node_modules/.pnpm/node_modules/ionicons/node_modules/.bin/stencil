#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/R/dwf/node_modules/.pnpm/@stencil+core@4.27.2/node_modules/@stencil/core/bin/node_modules:/mnt/e/R/dwf/node_modules/.pnpm/@stencil+core@4.27.2/node_modules/@stencil/core/node_modules:/mnt/e/R/dwf/node_modules/.pnpm/@stencil+core@4.27.2/node_modules/@stencil/node_modules:/mnt/e/R/dwf/node_modules/.pnpm/@stencil+core@4.27.2/node_modules:/mnt/e/R/dwf/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/R/dwf/node_modules/.pnpm/@stencil+core@4.27.2/node_modules/@stencil/core/bin/node_modules:/mnt/e/R/dwf/node_modules/.pnpm/@stencil+core@4.27.2/node_modules/@stencil/core/node_modules:/mnt/e/R/dwf/node_modules/.pnpm/@stencil+core@4.27.2/node_modules/@stencil/node_modules:/mnt/e/R/dwf/node_modules/.pnpm/@stencil+core@4.27.2/node_modules:/mnt/e/R/dwf/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@stencil+core@4.27.2/node_modules/@stencil/core/bin/stencil" "$@"
else
  exec node  "$basedir/../../../../../@stencil+core@4.27.2/node_modules/@stencil/core/bin/stencil" "$@"
fi
